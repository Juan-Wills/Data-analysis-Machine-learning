     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import numpy as np\n",
    "np.set_printoptions(precision=4, suppress=True)\n",
    "\n",
    "rng= np.random.default_rng(seed=12345)\n",
    "\n",
    "nsteps= 1000\n",
    "\n",
    "draws= rng.integers(0, 2, size= nsteps)\n",
    "\n",
    "steps= np.where(draws == 0, 1, -1)\n",
    "\n",
    "walk= steps.cumsum()\n",
    "\n",
    "walk.max()"
   ]
  },
